.login-container {
  background-color: #f77e5e;
  height: 100vh;
  width: 100vw;
  display: flex;
  flex-direction: column;
  align-items: center;
  color: #fff;
}
.login-container image {
  width: 204rpx;
  height: 188rpx;
  margin: 20rpx;
}
.login-container h6 {
  margin-bottom: 40rpx;
}
.login-container .inputBox {
  width: 80vw;
  margin: 20rpx;
  height: 60rpx;
}
.login-container .inputBox input {
  display: inline-block;
  width: 80%;
  font-size: 26rpx;
}
.login-container .inputBox .iconfont {
  display: inline-block;
  margin: 10rpx 20rpx;
  font-size: 44rpx;
  vertical-align: text-bottom;
}
.login-container .inputBox::after {
  display: block;
  content: "";
  height: 2rpx;
  background-color: #fff;
}
.login-container button {
  margin-top: 20rpx;
  width: 80vw !important;
  height: 70rpx !important;
  border-radius: 16rpx;
  background-color: #f7ce74;
  color: #ffffff;
  font-size: 32rpx;
  vertical-align: top;
  text-align: center;
}
.login-container .button-hover {
  opacity: 0.7;
}
.login-container .bottom {
  margin-top: 10rpx;
  width: 80vw;
  display: flex;
  justify-content: space-between;
}
.login-container .bottom span::after {
  display: block;
  content: "";
  height: 2rpx;
  background-color: #fff;
}
