.container {
  background-color: #f77e5e;
  height: 100vh;
  /* margin: 0*2rpx; */
}
/* 个人中心 */
.center {
  position: absolute;
  left: 40rpx;
  top: 64rpx;
  width: 70rpx;
  height: 70rpx;
  box-shadow: 0rpx 4rpx 12rpx 0rpx #674848;
  border: 2rpx solid #ffffff;
  display: inline-block;
  border-radius: 70rpx;
  background-color: #ffffff;
}
.center > .iconfont {
  display: inline-block;
  margin: 50%;
  transform: translate(-50%, -50%);
  font-size: 40rpx;
}
/* 顶部导航，上边距+下边距+内容高度=37+25+28=90 */
.top-bar {
  margin: 74rpx auto 50rpx;
}
/* 主内容 */
.content {
  border-radius: 100rpx 100rpx 0rpx 0rpx;
  height: calc(100vh - 90*2rpx);
  overflow: hidden;
  background-color: #ffffff;
}
